<!DOCTYPE html>
<html><$
sectionOrder=(
  plan
  runs
  results
  chart
)
sectionActive=plan
$>
  <head>
    <meta charset="utf-8">
    <title>3X for eXecuting eXploratory eXperiments</title>
    <link rel="stylesheet" type="text/css" href="resource/3x.css">
    <script type="text/javascript" src="resource/jquery.js"></script>
    <script type="text/javascript" src="resource/bootstrap.js"></script>
    <script type="text/javascript" src="resource/knockout.js"></script>
    <script type="text/javascript" src="resource/jquery.appear.js"></script>
    <script type="text/javascript" src="resource/jsrender.js"></script>
    <script type="text/javascript" src="resource/jquery.dataTables.js"></script>
    <script type="text/javascript" src="resource/jquery.dataTables.type-detection.num-html.js"></script>
    <script type="text/javascript" src="resource/jquery.dataTables.sorting.num-html.js"></script>
    <script type="text/javascript" src="resource/jquery.dataTables.bootstrap.js"></script>
    <script type="text/javascript" src="resource/jquery.dataTables.ColReorder.js"></script>
    <script type="text/javascript" src="resource/jquery.dataTables.Scroller.js"></script>
    <script type="text/javascript" src="resource/jquery.ui.core.js"></script>
    <script type="text/javascript" src="resource/jquery.ui.widget.js"></script>
    <script type="text/javascript" src="resource/jquery.ui.mouse.js"></script>
    <script type="text/javascript" src="resource/jquery.ui.sortable.js"></script>
    <script type="text/javascript" src="resource/jquery.ui.selectable.js"></script>
    <script type="text/javascript" src="resource/underscore.js"></script>
    <script type="text/javascript" src="resource/d3.js"></script>
    <script type="text/javascript" src="resource/numeral.js"></script>
    <script type="text/javascript" src="resource/socket.io.js"></script>
    <script type="text/javascript" src="resource/3x.js"></script>
    <base target="_3X_Inspector">
  </head>

  <body>

    <div class="navbar navbar-fixed-top">
      <div class="navbar-inner">
        <div class="container-fluid">
          <div>
            <a id="logo" class="brand" href="http://netj.github.com/3x/"><i
                class="icon-beaker icon"></i>3X</a>
          </div>
          <div class="pull-right">
            <span id="title" class="brand"></span>
          </div>

          <ul class="nav nav-tabs">
            <$ for section in "${sectionOrder[@]}"; do
              sectionBody=sections/"$section"/body.html
              [ -r "$sectionBody" ] || continue
              case $section in
                $sectionActive) classAttr=' class="active"' ;;
                             *) classAttr=
              esac
              $>
              <li<$= "$classAttr" $>><a data-toggle="tab" href="#<$= "$section" $>"><$
                sectionTab=sections/"$section"/tab.html
                if [ -r "$sectionTab" ]; then
                  cat "$sectionTab"
                else
                  echo "$section"
                fi
              $></a></li>
            <$ done $>
          </ul>
        </div>
      </div>
    </div>

    <div id="main" class="container-fluid tab-content" style="overflow-x: hidden;">

    <$ for section in "${sectionOrder[@]}"; do
      sectionBody=sections/"$section"/body.html
      [ -r "$sectionBody" ] || continue
      case $section in
        $sectionActive) classAttr=' active' ;;
                     *) classAttr=
      esac
      $>
<!-- <$= "$section" $> section begins - - - - - - - - - - - - - - - - - - - -->
<section id="<$= "$section" $>" class="tab-pane row-fluid<$= "$classAttr" $>">
<$ cat "$sectionBody" $>
</section>
<!-- <$= "$section" $> section ends - - - - - - - - - - - - - - - - - - - - -->
    <$ done $>

    </div>

  </body>
</html>
<!-- vim:set sw=2:sts=2:spell -->
